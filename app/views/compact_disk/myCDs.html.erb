<% content_for :title, "CD Sammlung" %>
<div class="row">
<div class="pull-right"><%= link_to t('.cd_new'), new_compact_disk_path %></div>
</div>
<ul id="selectable">
<% @myCDs.each do |c| %>
	<li>

		<%= image_tag c.photo.url(:normal) %>
		<p class="blackFont"><%= c.artist %></p>
		<p><%= c.title %></p>
		<p>
			<%= link_to 'Edit', edit_compact_disk_path(c) %>
			<%= link_to 'Show', compact_disk_path(c) %>
		</p>
	</li>
<% end %>
</ul>
